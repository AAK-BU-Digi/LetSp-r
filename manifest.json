{
   "update_url":"https://clients2.google.com/service/update2/crx",
   "manifest_version":3,
   "name":"LetSpær",
   "version":"0.9.2",
   "description":"Spærring af irrelevante sider i elevernes browser",
   "permissions":[
      "webRequest",
      "webRequestBlocking",
      "storage.managed"
   ],
   "background":{
      "service_worker":"background.js"
   },
   "storage":{
      "managed_schema":"schema.json"
   },
   "host_permissions":[
      "<all_urls>"
   ],
   "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["ai-mode-block.js"], 
      "run_at": "document_start"
    }
  ]
}
